<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
 <ImageView
     android:id="@+id/zdjecie1"
     android:layout_width="0dp"
     android:layout_height="wrap_content"
     android:src="@drawable/photo"
     app:layout_constraintBottom_toBottomOf="parent"
     app:layout_constraintStart_toStartOf="parent"
     app:layout_constraintEnd_toStartOf="@id/zdjecie2"
     android:adjustViewBounds="true"/>
    <ImageView
        android:id="@+id/zdjecie2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:src="@drawable/photo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/zdjecie1"
        app:layout_constraintEnd_toStartOf="@id/zdjecie3"
        android:adjustViewBounds="true"/>
    <ImageView
        android:id="@+id/zdjecie3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:src="@drawable/photo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/zdjecie2"
        app:layout_constraintEnd_toEndOf="parent"
        android:adjustViewBounds="true"/>

</androidx.constraintlayout.widget.ConstraintLayout>